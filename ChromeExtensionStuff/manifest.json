{
  "name": "HalalTube",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "main.html",
    "default_icon": "images/halalPic.png"
  },
  "icons": {
    "16": "images/halalPic.png",
    "48": "images/halalPic.png",
    "128": "images/halalPic.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "scripting"
  ]
}